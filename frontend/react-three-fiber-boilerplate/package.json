{
  "name": "react-three-fiber-boilerplate",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@react-three/fiber": "^8.13.7",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "add": "^2.0.6",
    "axios": "^1.5.0",
    "fabric": "^5.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-loading-icons": "^1.1.0",
    "react-scripts": "^5.0.1",
    "react-zoom-pan-pinch": "^3.1.0",
    "sats-connect": "^1.0.0",
    "three": "^0.156.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build:styles": "tailwindcss -i src/styles/tailwind.css -o src/styles/styles.css",
    "watch:styles": "tailwindcss -i src/styles/tailwind.css -o src/styles/styles.css --watch",
    "start:dev": "npm-run-all -p start watch:styles",
    "prebuild": "npm run build:styles",
    "prestart": "npm run build:styles"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.15",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.29",
    "tailwindcss": "^3.3.3"
  }
}
